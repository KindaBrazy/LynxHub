{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "electron.vite.extension.config.*",
    "src/main/**/*",
    "src/preload/**/*",
    "src/cross/**/*",
    "extension/src/main/**/*",
    "extension/src/cross/**/*",
    "module/src/**/*"
  ],
  "references": [
    {
      "path": "./module/tsconfig.json"
    }
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "electron-vite/node"
    ],
    "paths": {
      "@lynx_module/*": [
        "./module/src/*"
      ],
      "@lynx_extension/*": [
        "./extension/src/*"
      ]
    }
  }
}
