{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "paths": {
      "@lynx_module/*": [
        "./src/*"
      ],
      "@lynx_extension/*": [
        "../extension/src"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}