{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/**/*",
    "src/cross/**/*",
    "extension/src/renderer/**/*",
    "extension/src/cross/**/*",
    "module/src/**/*"
  ],
  "references": [
    {
      "path": "./module/tsconfig.json"
    }
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@lynx_module/*": [
        "module/src/*"
      ],
      "@lynx_extension/*": [
        "extension/src/*"
      ]
    }
  }
}
